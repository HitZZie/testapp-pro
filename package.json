{
  "name": "testapp-pro",
  "version": "1.0.0",
  "description": "Sistema de oposiciones con IA - TestApp Pro",
  "author": "Javi",
  "private": true,
  "homepage": "https://HitZZie.github.io/testapp-pro",
  "main": "public/main.js",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "firebase": "^11.10.0",
    "googleapis": "^153.0.0",
    "pdfjs-dist": "^5.3.93",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "build-electron": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --publish=never",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "electron": "^37.2.0",
    "electron-builder": "^26.0.12",
    "fs-extra": "^11.3.0",
    "gh-pages": "^6.3.0",
    "path": "^0.12.7",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.testapp.pro",
    "productName": "TestApp Pro",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "public/main.js",
      "node_modules/**/*"
    ],
    "win": {
      "target": "portable"
    },
    "portable": {
      "artifactName": "TestApp-Pro-Portable.exe"
    }
  }
}
